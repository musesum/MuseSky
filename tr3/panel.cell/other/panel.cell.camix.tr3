panel.cell.camix: _camera {

    base {
        type: "camix"
        title: "Camera Mix"
        icon: "icon.camera.png"
    }
    controls {
        cameraOn.value -> sky.shader.cellCamix.on
        version.value: (0...1=0.5) -> sky.shader.cellCamix.buffer.version 
        bitplane.value: (0...1=0) -> sky.shader.colorize.buffer.bitplane
    }
}
